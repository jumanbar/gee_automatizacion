makeUpdate <- function(time_start, id_zona, percentil, valor) {
paste0("UPDATE 7_zonas SET valor = ", valor,
" WHERE time_start = '", time_start,
"' AND id_zona = ", id_zona,
" AND percentil = ", percentil,
" AND id_parametro = 2000;")
}
